{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/create-room",
      "methods": ["POST"],
      "dest": "server/index.js"
    },
    {
      "src": "/heartbeat",
      "methods": ["POST"],
      "dest": "server/index.js"
    },
    {
      "src": "/get-agora-token",
      "methods": ["POST", "GET"],
      "dest": "server/index.js"
    },
    {
      "src": "/health",
      "dest": "server/index.js"
    },
    {
      "src": "/test-token",
      "dest": "server/index.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}